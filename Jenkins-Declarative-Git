pipeline{
    agent any
    environment{
        PATH = "/opt/maven3/bin:$PATH"
    }
    stages{
        stage("git pull"){
            steps{
                git 'https://github.com/ybsgit/myweb'
            }
        }
        stage("maven build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
